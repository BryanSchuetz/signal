---
title: Noise
pageable: true
---

<% page_articles.each_with_index do |article, i| %>
  <article class="<%= (i == 0) ? 'lead' : '' %>">
    <h2><a href="<%= article.url %>"><%= article.title %></a></h2>
    <span class="postDetails clearfix"><time datetime="<%= article.date.strftime('%m-%Y') %>" class="date details"><%= article.date.strftime('%b %e %Y') %></time></span>
    <%= article.summary %>
    <p class="readmore details"><a href="<%= article.url %>"<i class="ss-icon">&#x21AA;</i> Read More</a></p>
  </article>
  <br>
<% end %>
  <%= partial "paginate", :locals => {
  :paginate => paginate,
  :next_page => next_page,
  :prev_page => prev_page,
  :num_pages => num_pages,
  :page_number => page_number
} %>
